<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!--
    Dog dog = new Dog();

    dog.setName("멍멍이");

    <bean id="dog" class="com.beyond.di.pet.Dog">
        <property name="name" value="멍멍이"/>
    </bean>
    -->
    <bean id="dog" class="com.beyond.di.pet.Dog" p:name="멍멍이"/>

    <!--
    Cat cat = new Cat("야옹이");

    <bean id="cat" class="com.beyond.di.pet.Cat">
        <constructor-arg name="name" value="야옹이"/>
    </bean>

    <bean id="cat" class="com.beyond.di.pet.Cat" c:name="야옹이"/>
    <bean id="cat" class="com.beyond.di.pet.Cat" c:_0="야옹이"/>
    -->
    <bean id="cat" class="com.beyond.di.pet.Cat" c:_="야옹이"/>

</beans>